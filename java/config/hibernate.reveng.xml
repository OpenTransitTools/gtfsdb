<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "lib/hibernate-build-tools/hibernate-reverse-engineering-3.0.dtd" >

<hibernate-reverse-engineering>

<!--
  <schema-selection match-schema="public schema (or something...default set in hmb.xml, so not needed)"/>
-->
  <table-filter match-name="agency"/>
  <table-filter match-name="calendar"/>
  <table-filter match-name="calendar_dates"/>
  <table-filter match-name="fare_attributes"/>
  <table-filter match-name="fare_rules"/>
  <table-filter match-name="frequencies"/>
  <table-filter match-name="patterns"/>
  <table-filter match-name="route_type"/>
  <table-filter match-name="routes"/>
  <table-filter match-name="stop_times"/>
  <table-filter match-name="stops"/>
  <table-filter match-name="transfers"/>
  <table-filter match-name="trips"/>
  <table-filter match-name="universal_calendar"/>

  <table name="Patterns">
    <column name="geom" exclude="true" />
  </table>

<!--
  <schema-selection match-schema="another schema (maybe 2nd agency ...)"/>
-->

</hibernate-reverse-engineering>
